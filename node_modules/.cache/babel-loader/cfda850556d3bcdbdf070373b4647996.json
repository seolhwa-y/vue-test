 </el-button>\n          <!-- 수정 -->\n          <el-button class=\"el-icon-edit\" circle @click=\"revise()\">\n            <el-icon :size=\"20\" :color=\"color\"> <EditPen /> </el-icon>\n          </el-button>\n          <el-button type=\"info\" circle @click=\"remove()\">\n            <el-icon :size=\"20\" :color=\"color\"> <Delete /> </el-icon>\n          </el-button>\n        </el-col>\n        <el-col :span=\"16\">\n          <el-row class=\"date\">\n            <p>{{ formatCardDate(lists.d_diarydt) }}</p>\n            <!-- {{ lists.d_diarydt }} //20240217 -->\n          </el-row>\n\n          <el-row>\n            <span class=\"title\">{{ lists.d_title }}</span>\n            <span class=\"icon\">\n              <el-icon v-if=\"lists.d_weather === '1'\"><Sunny /></el-icon>\n              <el-icon v-else-if=\"lists.d_weather === '2'\"\n                ><MostlyCloudy\n              /></el-icon>\n              <el-icon v-else-if=\"lists.d_weather === '3'\"><Pouring /></el-icon>\n              <el-icon v-else-if=\"lists.d_weather === '4'\"\n                ><Lightning\n              /></el-icon>\n              (<span v-if=\"lists.d_mood === '기쁨'\">\n                <img src=\"/images/happy.png\" alt=\"기쁨\" />\n                <span>{{ lists.d_mood }}</span>\n              </span>\n              <span v-else-if=\"lists.d_mood === '평범'\">\n                <img src=\"/images/weird.png\" alt=\"평범\" />\n                <span>{{ lists.d_mood }}</span>\n              </span>\n              <span v-else-if=\"lists.d_mood === '우울'\">\n                <img src=\"/images/sad.png\" alt=\"우울\" />\n                <span>{{ lists.d_mood }}</span>\n              </span>\n              <span v-else-if=\"lists.d_mood === '화남'\">\n                <img src=\"/images/anger.png\" alt=\"화남\" />\n                <span>{{ lists.d_mood }}</span>\n              </span>\n              <span v-else-if=\"lists.d_mood === '놀람'\">\n                <img src=\"/images/shock.png\" alt=\"놀람\" />\n                <span>{{ lists.d_mood }}</span> </span\n              >)</span\n            >\n          </el-row>\n        </el-col>\n      </el-row>\n\n      <el-row>\n        <el-row justify=\"center\">\n          <el-col :span=\"24\">\n            <el-divider></el-divider>\n          </el-col>\n        </el-row>\n\n        <div justify=\"center\" class=\"content\">\n          <PureText>{{ lists.d_contents }}</PureText>\n        </div>\n      </el-row>\n    </el-row>\n  </div>\n</template>\n\n<script>\nimport { computed } from \"vue\";\n\nexport default {\n  props: {\n    d_no: String,\n    htmlContent: String, // HTML 태그를 포함한 내용을 받아올 prop\n  },\n  setup(props) {\n    // HTML 태그를 제거한 순수한 텍스트를 계산된 속성으로 반환\n    const pureTextContent = computed(() => {\n      const tempElement = document.createElement(\"div\");\n      tempElement.innerHTML = props.htmlContent;\n      return tempElement.textContent || tempElement.innerText || \"\";\n    });\n\n    return {\n      pureTextContent,\n    };\n  },\n  name: \"ListByNo\",\n  data() {\n    return {\n      lists: [],\n      d_title: \"\",\n      d_contents: \"\",\n      d_weather: \"\",\n      d_mood: \"\",\n      d_diarydt: \"\",\n      pd_no: \"\",\n    };\n  },\n\n  mounted() {\n    // this.pd_title = this.d_title;\n    this.pd_no = this.$route.query.d_no;\n    // this.pd_contents = this.d_contents;\n    // this.pd_weather = this.d_weather;\n    // this.pd_mood = this.d_mood;\n    // this.pd_diarydt = this.d_diarydt;\n\n    let vm = this;\n\n    let params = new URLSearchParams();\n    params.append(\"d_no\", this.pd_no);\n    console.log(\"pd_no\", this.pd_no);\n\n    this.axios.post(\"/diary/detail.do\", params).then(function (response) {\n      console.log(response.data.result);\n      // HTML 태그를 제거한 순수한 텍스트 추출\n      const dContents = response.data.result.d_contents;\n      // HTML 태그를 제거한 순수한 텍스트 추출\n      const pureText = dContents.replace(/<[^>]+>/g, \"\");\n\n      vm.lists = {\n        d_no: response.data.result.d_no,\n        d_diarydt: response.data.result.d_diarydt,\n        d_mood: response.data.result.d_mood,\n        d_title: response.data.result.d_title,\n        d_weather: response.data.result.d_weather,\n        d_contents: pureText, // HTML 태그가 제거된 순수한 텍스트로 설정\n      };\n    });\n  },\n  methods: {\n    back() {\n      this.$router.push(\"/dashboard/diary/list\");\n    },\n\n    revise() {\n      this.$router.push({\n        path: \"/dashboard/diary/DiaryInsert\",\n        query: {\n          action: \"U\",\n          d_no: this.$route.query.d_no,\n        },\n      });\n    },\n\n    remove() {\n      if (confirm(\"삭제 하시겠습니까?\")) {\n        let params = new URLSearchParams();\n        params.append(\"d_no\", this.pd_no);\n\n        // 화살표 함수를 사용하여 콜백 함수 내의 this를 컴포넌트 자체로 설정\n        this.axios\n          .post(\"/diary/delete.do\", params)\n          .then((response) => {\n            console.log(response);\n            this.$message({\n              type: \"info\",\n              message: \"일기를 삭제하였습니다.\",\n            });\n            this.$router.push(\"/dashboard/diary/list\");\n          })\n          .catch((error) => {\n            console.error(\"Error deleting diary:\", error);\n          });\n      }\n    },\n    formatCardDate(date) {\n      if (!date || date.length !== 8) {\n        return \"날짜를 가져올 수 없습니다.\";\n      }\n      // 문자열을 연, 월, 일로 분할\n      const year = date.substring(0, 4);\n      const month = date.substring(4, 6);\n      const day = date.substring(6, 8);\n\n      // 월과 일에 대해 0을 제거\n      const formattedMonth = parseInt(month, 10).toString();\n      const formattedDay = parseInt(day, 10).toString();\n\n      // 연도, 월, 일을 조합하여 원하는 형식으로 반환\n      return `${year}년 ${formattedMonth}월 ${formattedDay}일`;\n    },\n  },\n};\n</script>\n\n<style scoped>\n.post-detail {\n  margin: 2% auto;\n  max-width: 800px;\n  padding: 20px;\n  background-color: #fff;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n}\n.content-top {\n  padding: auto;\n}\n.date {\n  color: gray;\n  font-size: 20px;\n  font-weight: bold;\n}\n\n.title {\n  margin-top: 4%;\n  font-size: 20px;\n}\n\n.icon {\n  color: gray;\n  margin-top: 2%;\n  font-size: 15px;\n}\n\nimg {\n  width: 30px;\n  height: 30px;\n}\n\n.flex-container {\n  display: flex;\n  justify-content: flex-end;\n}\n.content {\n  height: 500px;\n  overflow-y: auto;\n  overflow-x: auto;\n  width: 100%;\n  font-size: 15px;\n}\n.el-row {\n  flex-direction: column;\n}\n</style>\n"],"mappings":"AA2EA,SAASA,QAAO,QAAS,KAAK;AAE9B,eAAe;EACbC,KAAK,EAAE;IACLC,IAAI,EAAEC,MAAM;IACZC,WAAW,EAAED,MAAM,CAAE;EACvB,CAAC;EACDE,KAAKA,CAACJ,KAAK,EAAE;IACX;IACA,MAAMK,eAAc,GAAIN,QAAQ,CAAC,MAAM;MACrC,MAAMO,WAAU,GAAIC,QAAQ,CAACC,aAAa,CAAC,KAAK,CAAC;MACjDF,WAAW,CAACG,SAAQ,GAAIT,KAAK,CAACG,WAAW;MACzC,OAAOG,WAAW,CAACI,WAAU,IAAKJ,WAAW,CAACK,SAAQ,IAAK,EAAE;IAC/D,CAAC,CAAC;IAEF,OAAO;MACLN;IACF,CAAC;EACH,CAAC;EACDO,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,UAAU,EAAE,EAAE;MACdC,SAAS,EAAE,EAAE;MACbC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE,EAAE;MACbC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACD,KAAI,GAAI,IAAI,CAACE,MAAM,CAACC,KAAK,CAACtB,IAAI;IACnC;IACA;IACA;IACA;;IAEA,IAAIuB,EAAC,GAAI,IAAI;IAEb,IAAIC,MAAK,GAAI,IAAIC,eAAe,CAAC,CAAC;IAClCD,MAAM,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACP,KAAK,CAAC;IACjCQ,OAAO,CAACC,GAAG,CAAC,OAAO,EAAE,IAAI,CAACT,KAAK,CAAC;IAEhC,IAAI,CAACU,KAAK,CAACC,IAAI,CAAC,kBAAkB,EAAEN,MAAM,CAAC,CAACO,IAAI,CAAC,UAAUC,QAAQ,EAAE;MACnEL,OAAO,CAACC,GAAG,CAACI,QAAQ,CAACpB,IAAI,CAACqB,MAAM,CAAC;MACjC;MACA,MAAMC,SAAQ,GAAIF,QAAQ,CAACpB,IAAI,CAACqB,MAAM,CAAClB,UAAU;MACjD;MACA,MAAMoB,QAAO,GAAID,SAAS,CAACE,OAAO,CAAC,UAAU,EAAE,EAAE,CAAC;MAElDb,EAAE,CAACV,KAAI,GAAI;QACTb,IAAI,EAAEgC,QAAQ,CAACpB,IAAI,CAACqB,MAAM,CAACjC,IAAI;QAC/BkB,SAAS,EAAEc,QAAQ,CAACpB,IAAI,CAACqB,MAAM,CAACf,SAAS;QACzCD,MAAM,EAAEe,QAAQ,CAACpB,IAAI,CAACqB,MAAM,CAAChB,MAAM;QACnCH,OAAO,EAAEkB,QAAQ,CAACpB,IAAI,CAACqB,MAAM,CAACnB,OAAO;QACrCE,SAAS,EAAEgB,QAAQ,CAACpB,IAAI,CAACqB,MAAM