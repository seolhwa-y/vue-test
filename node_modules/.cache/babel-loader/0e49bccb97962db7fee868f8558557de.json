ble id=\"grpInfo\">\n      <caption>\n        caption\n      </caption>\n      <colgroup>\n        <col width=\"120px\" />\n        <col width=\"*\" />\n        <col width=\"120px\" />\n        <col width=\"*\" />\n      </colgroup>\n\n      <tbody>\n        <tr>\n          <td colspan=\"4\" class=\"text-center\">\n            <div class=\"my-4\">\n              <strong style=\"font-size: 30px\">{{ title }}</strong>\n            </div>\n          </td>\n        </tr>\n\n        <tr>\n          <th scope=\"row\">그룹 코드 <span class=\"font_red\">*</span></th>\n          <td>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              name=\"grp_cod\"\n              id=\"grp_cod\"\n              v-model=\"grp_cod\"\n            />\n          </td>\n          <th scope=\"row\">그룹 코드 명 <span class=\"font_red\">*</span></th>\n          <td>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              name=\"grp_cod_nm\"\n              id=\"grp_cod_nm\"\n              v-model=\"grp_cod_nm\"\n            />\n          </td>\n        </tr>\n        <tr>\n          <th scope=\"row\">코드 설명</th>\n          <td colspan=\"3\">\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              name=\"grp_cod_eplti\"\n              id=\"grp_cod_eplti\"\n              v-model=\"grp_cod_eplti\"\n            />\n          </td>\n        </tr>\n\n        <tr>\n          <th scope=\"row\">사용 유무 <span class=\"font_red\">*</span></th>\n          <td colspan=\"3\">\n            <input\n              type=\"radio\"\n              name=\"grp_use_poa\"\n              id=\"grp_use_poa_1\"\n              value=\"Y\"\n              v-model=\"grp_use_poa\"\n            />\n            <label for=\"radio1-1\">사용</label>\n            &nbsp;&nbsp;&nbsp;&nbsp;\n            <input\n              type=\"radio\"\n              name=\"grp_use_poa\"\n              id=\"grp_use_poa_2\"\n     