AAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;AACnC","file":"/Users/shy/vue-workspace/vue-test/src/components/system/grddetailModal.vue","sourceRoot":"","sourcesContent":["<template>\n  <dl id=\"grpInfoWrap\">\n    <dd class=\"content\"></dd>\n    <!-- s : 여기에 내용입력 -->\n    <table id=\"grpInfo\">\n      <caption>\n        caption\n      </caption>\n      <colgroup>\n        <col width=\"120px\" />\n        <col width=\"*\" />\n        <col width=\"120px\" />\n        <col width=\"*\" />\n      </colgroup>\n\n      <tbody>\n        <tr>\n          <td colspan=\"4\" class=\"text-center\">\n            <div class=\"my-4\">\n              <strong style=\"font-size: 30px\">{{ title }}</strong>\n            </div>\n          </td>\n        </tr>\n\n        <tr>\n          <th scope=\"row\">그룹 코드 <span class=\"font_red\">*</span></th>\n          <td>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              name=\"grp_cod\"\n              id=\"grp_cod\"\n              v-model=\"grp_cod\"\n            />\n          </td>\n          <th scope=\"row\">그룹 코드 명 <span class=\"font_red\">*</span></th>\n          <td>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              name=\"grp_cod_nm\"\n              id=\"grp_cod_nm\"\n              v-model=\"grp_cod_nm\"\n            />\n          </td>\n        </tr>\n        <tr>\n          <th scope=\"row\">코드 설명</th>\n          <td colspan=\"3\">\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              name=\"grp_cod_eplti\"\n              id=\"grp_cod_eplti\"\n              v-model=\"grp_cod_eplti\"\n            />\n          </td>\n        </tr>\n\n        <tr>\n          <th scope=\"row\">사용 유무 <span class=\"font_red\">*</span></th>\n          <td colspan=\"3\">\n            <input\n              type=\"radio\"\n              name=\"grp_use_poa\"\n              id=\"grp_use_poa_1\"\n              value=\"Y\"\n              v-model=\"grp_use_poa\"\n            />\n            <label for=\"radio1-1\">사용</label>\n            &nbsp;&nbsp;&nbsp;&nbsp;\n            <input\n              type=\"radio\"\n              name=\"grp_use_poa\"\n              id=\"grp_use_poa_2\"\n              value=\"N\"\n              v-model=\"grp_use_poa\"\n            />\n            <label for=\"radio1-2\">미사용</label>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n\n    <!-- e : 여기에 내용입력 -->\n\n    <div class=\"btn_areaC mt30\">\n      <a @click=\"save()\" class=\"btn btn-primary\" id=\"btnSaveGrpCod\" name=\"btn\">\n        <span>저장</span>\n      </a>\n      <a @click=\"deletefrpcd()\" class=\"btn btn-danger mx-2\" v-show=\"delshow\">\n        <span>삭제</span>\n      </a>\n    </div>\n  </dl>\n</template>\n\n<script>\nimport { closeModal } from \"jenesius-vue-modal\";\nexport default {\n  // vue에서는 받아온 변수를 methods에서 직접 핸들링이 불가능하기 때문에\n  // 임시 변수를 만들어서 받아온 변수를 넣어 줘야 함\n  props: { title: String, grpcd: String, action: String },\n  data: function () {\n    return {\n      pgrpcd: this.grpcd,\n      grp_cod: \"\",\n      grp_cod_nm: \"\",\n      grp_cod_eplti: \"\",\n      grp_use_poa: \"\",\n      delshow: false,\n      eventYn: \"\",\n    };\n  },\n  computed: {\n    agrpcd: {\n      get: function () {\n        return this.data.pgrpcd;\n      },\n      set: function (v) {\n        this.data.pgrpcd = v;\n      },\n    },\n  },\n  // html 로딩, 가상 dom 실행, 이 두 개 연결 시 작동\n  mounted: function () {\n    let vm = this;\n    // 신규 등록 시\n    if (this.grpcd == null || this.grpcd == \"\") {\n      vm.grp_cod = \"\";\n      vm.grp_cod_nm = \"\";\n      vm.grp_cod_eplti = \"\";\n      vm.grp_use_poa = \"\";\n    } else {\n      //  수정 시 (grp_cod 에 해당하는 상세코드 정보 가져오기)\n      let params = new URLSearchParams();\n      params.append(\"grp_cod\", this.grpcd);\n\n      this.axios\n        .post(\"/system/selectComnGrpCod.do\", params)\n        .then(function (response) {\n          console.log(response);\n          // response data;\n\n          // detailcnt: 0\n          // fnl_mdfd_dtt: \"2022-03-25 15:09:14.0\"\n          // fnl_mdfr_sst_id: null\n          // fst_enlm_dtt: 1648177974000\n          // fst_rgst_sst_id: null\n          // grp_cod: \"shipCD\"\n          // grp_cod_eplti: null\n          // grp_cod_nm: \"배송코드\"\n          // reg_date: null\n          // row_num: 0\n          //         // tmp_fld_01: \"3\"\n          // tmp_fld_02: null\n          // tmp_fld_03: null\n          // use_poa: \"Y\"\n\n          vm.grp_cod = response.data.comnGrpCodModel.grp_cod;\n          vm.grp_cod_nm = response.data.comnGrpCodModel.grp_cod_nm;\n          vm.grp_cod_eplti = response.data.comnGrpCodModel.grp_cod_eplti;\n          vm.grp_use_poa = response.data.comnGrpCodModel.use_poa;\n\n          if (parseInt(response.data.comnGrpCodModel.tmp_fld_01) == 0) {\n            vm.delshow = true;\n          } else {\n            vm.delshow = false;\n          }\n        })\n        .catch(function (error) {\n          alert(\"에러! API 요청에 오류가 있습니다. \" + error);\n        });\n    }\n  },\n  methods: {\n    save: function () {\n      if (confirm(\"저장하시겠습니까?\")) {\n        let vm = this;\n        let params = new URLSearchParams();\n\n        params.append(\"grp_cod\", this.grp_cod);\n        params.append(\"grp_cod_nm\", this.grp_cod_nm);\n        params.append(\"grp_cod_eplti\", this.grp_cod_eplti);\n        params.append(\"grp_use_poa\", this.grp_use_poa);\n        params.append(\"action\", this.action);\n\n        this.axios\n          .post(\"/system/saveComnGrpCod.do\", params)\n          .then(function (response) {\n            console.log(response);\n            let status = response.status;\n            let msg = response.data.resultMsg;\n            if (status == 200) {\n              alert(msg);\n              closeModal(vm);\n            } else {\n              alert(msg);\n            }\n          })\n          .catch(function (error) {\n            alert(\"에러! API 요청에 오류가 있습니다. \" + error);\n          });\n      }\n    },\n    deletefrpcd: function () {\n      let vm = this;\n      let params = new URLSearchParams();\n      params.append(\"grp_cod\", this.grp_cod);\n\n      this.axios\n        .post(\"/system/deleteComnGrpCod.do\", params)\n        .then(function (response) {\n          console.log(response);\n          alert(response.data.resultMsg);\n          closeModal(vm);\n        })\n        .catch(function (error) {\n          alert(\"에러! API 요청에 오류가 있습니다. \" + error);\n        });\n    },\n  },\n  created() {\n    // 자식요소에서 부모 요소 method 호출\n  },\n};\n</script>\n\n<style>\n#grpInfo {\n  border-collapse: separate;\n  border-spacing: 20px;\n}\n#grpInfoWrap {\n  background-color: #fff;\n  padding: 3rem;\n  border-radius: 10px;\n  border: 2px solid rgb(59, 59, 59);\n}\n</style>\n"]}]}